//
// named.conf
//
// Provided by Red Hat bind package to configure the ISC BIND named(8) DNS
// server as a caching only nameserver (as a localhost DNS resolver only).
//
// See /usr/share/doc/bind*/sample/ for example named configuration files.
//

acl trust_public {
            120.50.128.0/19; 124.137.96.0/21;
            203.226.253.0/24; 203.226.254.0/23; 211.115.10.0/23;
            211.234.239.0/24; 211.234.240.0/22; 61.97.64.0/20;
            117.53.96.0/19; 61.100.11.192/27; 121.156.73.0/23;
      };
acl trust_private {
            10.0.0.0/8; 172.18.0.0/16; 172.19.0.0/16; 192.168.0.0/16;
            127.0.0.1;
        };

options {
        listen-on port 53 { any; };
        listen-on-v6 port 53 { any; };
        directory       "/var/named";
        dump-file       "/var/named/data/cache_dump.db";
        version "Cyworld NAME :D";
        check-names master ignore ;
        check-names slave ignore ;
        transfer-format many-answers;
        statistics-file "/var/named/data/named_stats.txt";
        memstatistics-file "/var/named/data/named_mem_stats.txt";
        allow-query     { any; };
 //       allow-query     { trust_private; };

        dnssec-enable yes;
        dnssec-validation yes;
        dnssec-lookaside auto;

        /* Path to ISC DLV key */
        bindkeys-file "/etc/named.iscdlv.key";

        managed-keys-directory "/var/named/dynamic";

        also-notify {
                10.42.43.27; 10.42.43.24; 10.42.43.25;
                /* natecorp */
                192.168.184.121; 192.168.184.122;
        };
        allow-transfer {
                127.0.0.1; 10.42.43.24; 10.42.43.25; 10.42.43.27;
                /* natecorp */
                192.168.184.121; 192.168.184.122;
        };

        forwarders { 8.8.8.8; };

        allow-recursion { trust_public; trust_private; };
        recursive-clients 16384;

};

include "/etc/rndc.key";

controls {
      inet 127.0.0.1 port 953
      allow { 127.0.0.1; } keys { "rndc-key"; };
};

logging {
//        channel default_debug {
//                file "data/named.run";
//                severity dynamic;
//        };
        channel syslog {
//                syslog daemon;
                severity warning;
                file "/var/log/named/named.log" versions 10 size 20M;
                print-severity yes;
                print-category yes;
        };
        channel null {
                null;
        };
        category default { syslog; };

        category lame-servers { null; };
//        category cname { null; };
//        category response-checks { null; };
        category notify { null; };
        category update { null; };
};

zone "." IN {
        type hint;
        file "named.ca";
};

include "/etc/named.rfc1912.zones";
include "/etc/named.root.key";

